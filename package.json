{
  "name": "antd-react-extensions-nextjs",
  "version": "0.4.0",
  "license": "MIT",
  "repository": "https://github.com/wuifdesign/antd-react-extensions/tree/nextjs",
  "main": "dist/index.js",
  "source": "src/lib/index.tsx",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "sideEffects": [
    "dist/*",
    "dist/**/style/*",
    "*.less"
  ],
  "scripts": {
    "prepare": "husky install",
    "start": "npm run storybook",
    "build": "npm run clear-dist && npm run babel && npm run ts-declaration && npm run copy-less",
    "start-react-app": "react-scripts start",
    "build-react-app": "react-scripts build && npm run analyze-react-app",
    "analyze-react-app": "source-map-explorer 'build/static/js/*.js'",
    "clear-dist": "rimraf dist",
    "babel": "babel --extensions .ts,.tsx src/lib --out-dir dist",
    "ts-declaration": "tsc src/lib/index.tsx --declaration --emitDeclarationOnly --jsx react --esModuleInterop --outDir dist --skipLibCheck",
    "copy-less": "copyfiles -u 2 \"src/lib/**/*.less\" dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-storybook-log-to-file": "build-storybook -s public -o docs 2>> storybook-log.txt",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "react-scripts test --ci --watchAll=false",
    "test:lint": "eslint \"./src/**/*.{js,ts,tsx}\"",
    "test:lint:fix": "eslint \"./src/**/*.{js,ts,tsx}\" --fix",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.6.0",
    "antd": "^4.17.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@dnd-kit/core": "^5.0.1",
    "@dnd-kit/modifiers": "^5.0.0",
    "@dnd-kit/sortable": "^6.0.0",
    "clsx": "^1.1.1",
    "deepmerge": "^4.2.2",
    "papaparse": "^5.3.1",
    "react-dropzone": "^12.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.19",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@types/papaparse": "^5.3.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/tinycolor2": "^1.4.3",
    "@types/uuid": "^8.3.4",
    "antd": "^4.18.6",
    "babel-loader": "8.1.0",
    "copyfiles": "^2.4.1",
    "cypress": "^9.4.1",
    "eslint-config-wuif": "^0.0.7",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^7.3.0",
    "next": "^12.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2",
    "storybook-addon-next-router": "^3.1.1",
    "typescript": "4.4.*",
    "web-vitals": "^2.1.4",
    "webpack": "^4.44.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
